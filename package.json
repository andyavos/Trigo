{
  "name": "trigo",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build:css": "sass src/main.scss dist/styles.css --style=compressed --no-source-map --load-path=node_modules",
    "watch:css": "sass src/main.scss dist/styles.css --watch --style=expanded --load-path=node_modules",
    "build": "npm run build:css"
  },
  "devDependencies": {
    "sass": "^1.85.0"
  },
  "dependencies": {
    "minireset.css": "^0.0.7"
  }
}
